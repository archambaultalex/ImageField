{
    "name": "archambaultalex/ImageField",
    "description": "A Laravel Nova field used to display images",
    "authors": [
        {
            "name": "Alexandre Archambault",
            "role": "Developer"
        }
    ],
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Archambaultalex\\Base64Image\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Archambaultalex\\ImageField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
